{
  "name": "react-tetris2",
  "version": "0.1.0",
  "description": "A highly-configurable, accessible React Tetris component",
  "type": "module",
 "engines": {
        "node": ">=22.0.0"
     },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Ingenarte/react-tetris2.git"
  },
  "homepage": "https://ingenarte.github.io/react-tetris2/",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ingenarte/react-tetris2/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc --build tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "prepare": "husky install",
    "dev": "vite"
  },
  "dependencies": {
    "keymaster": "^1.6.2"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "eslint": "^8.50.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "vitest": "^3.1.3",
    "husky": "^9.0.0",
    "rimraf": "^5.0.0"
  }
}


